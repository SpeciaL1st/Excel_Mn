{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"linebreak-style": "off",
		"arrow-parens": "off",
		"indent": "off",
		"no-tabs": "off",
		"comma-dangle": "off",
		"no-undef": "off",
		"require-jsdoc": "off"
	},

	"extends": [
		"eslint:recommended", "google"
	]
}